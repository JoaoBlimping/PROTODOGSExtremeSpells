script/source = "extends KinematicBody2D

const speed = 200


func _ready():
	set_process(true)


func _process(delta):
	var vel = Vector2()
	if (Input.is_action_pressed(\"ui_left\")): vel.x = -speed
	if (Input.is_action_pressed(\"ui_right\")): vel.x = speed
	if (Input.is_action_pressed(\"ui_up\")): vel.y = -speed
	if (Input.is_action_pressed(\"ui_down\")): vel.y = speed
	move(vel * delta)
"


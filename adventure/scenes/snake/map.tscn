func cranberry():
	move(\"snake/cranberry\")

func seabreeze():
	move(\"snake/seabreeze\")

func eel():
	move(\"snake/pub\")

func antique():
	if (s(\"hammer\")): move(\"snake/antiqueMainRoom\")
	else: say(\"We should check into the motel first\")

func orange():
	if (s(\"thirdSleep\")): move(\"orange/outside\")
	elif (s(\"hammer\")): say(\"We ought to sleep first\")
	else: say(\"that's orange man's house, we need to worry about the hammer first\")"

